<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mx.edu.uteq.dapps.proyecto195">

    <uses-permission android:name="android.permission.INTERNET" />

    <!-- Permitir http -->
    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Proyecto195">
        <activity android:name=".EjemploWebServiceActivity"></activity>
        <activity
            android:name=".EjemploListviewActivity"
            android:label="Ejemplo ListView" />
        <activity
            android:name=".RegistroActivity"
            android:label="Registro" />
        <activity
            android:name=".LoginActivity"
            android:label="Baicatelas">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Proyecto195.NoActionBar" />
    </application>

</manifest>